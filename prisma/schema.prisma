generator kysely {
    provider = "prisma-kysely"
    output = "../src/db"
    fileName = "types.ts"
    enumFileName = "enums.ts"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  name  String
  email String?  @unique
}

